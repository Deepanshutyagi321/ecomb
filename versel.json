{
    "version": 3,
    "builds": [
      {
        "src": "!.**",
        "use": "@vercel/nft-storage"
      },
      {
        "src": "**/*.js",
        "use": "@vercel/node",
        "config": {
          "nodeVersion": "16"
        }
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/*"} 
    ]
  }